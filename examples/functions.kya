rec Foo {
    x: int,
    y: int,
    z: int
}

fun foo(baz: bool): Foo {
    let bar: int = 5;
    println(bar); % 5
    return Foo:init(
        x: 17,
        y: 500,
        z: 83
    );
}

fun main() {
    let x: Foo = foo(true);
    let a: int = x.y;
    let b: int = x.x;
    let c: int = x.z;
    println(a); % 500
    println(b); % 17
    println(c); % 83
}
